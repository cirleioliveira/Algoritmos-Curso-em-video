algoritmo"num"
var
   a: inteiro
   b, c: real
inicio
      a <- Abs(-50) // valor absoluto
      escreval(a)
      
      b <- Exp(9, 5)
      escreval(b)
      
      c <- RaizQ(81)
      escreval(c)
      
